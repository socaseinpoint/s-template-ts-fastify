{
  "name": "ts-service-template",
  "version": "1.0.0",
  "description": "TypeScript service template with ESLint, Prettier, and Husky",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "dotenv -e .env -- cross-env MODE=all tsx watch -r tsconfig-paths/register src/server.ts",
    "dev:api": "dotenv -e .env -- cross-env MODE=api tsx watch -r tsconfig-paths/register src/server.ts",
    "dev:worker": "dotenv -e .env -- cross-env MODE=worker tsx watch -r tsconfig-paths/register src/worker.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "MODE=api node -r tsconfig-paths/register dist/server.js",
    "start:worker": "MODE=worker node -r tsconfig-paths/register dist/worker.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run tests/unit",
    "test:watch": "vitest",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:full": "./scripts/test-e2e.sh",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run tests/unit --coverage --reporter=default --reporter=json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "clean": "rm -rf dist coverage .vitest",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "service",
    "template",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axios": "^1.12.2",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^14.0.0",
    "husky": "^8.0.3",
    "pino-pretty": "^10.3.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "tsc-alias": "^1.8.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/fastify": "^6.13.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/redis": "^7.0.2",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^5.7.0",
    "@types/ms": "^2.1.0",
    "awilix": "^11.0.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.0.0",
    "dotenv": "^16.0.0",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.0.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "prisma": "^5.7.0",
    "prom-client": "^15.1.3",
    "zod": "^4.1.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
