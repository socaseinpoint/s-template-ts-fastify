{
  "name": "ts-service-template",
  "version": "1.0.0",
  "description": "TypeScript service template with ESLint, Prettier, and Husky",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch -r tsconfig-paths/register src/server.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "start:dev": "tsx -r tsconfig-paths/register src/server.ts",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "precommit": "npm run lint:check && npm run format:check && npm run type-check && npm run test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:build": "docker compose -f docker-compose.dev.yml build",
    "docker:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:restart": "docker compose -f docker-compose.dev.yml restart",
    "docker:clean": "docker compose -f docker-compose.dev.yml down -v"
  },
  "keywords": [
    "typescript",
    "service",
    "template",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axios": "^1.12.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^14.0.0",
    "husky": "^8.0.3",
    "pino-pretty": "^10.3.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "tsc-alias": "^1.8.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/redis": "^7.0.2",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^5.7.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.0.0",
    "fastify": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
